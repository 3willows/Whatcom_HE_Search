{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@Pages/*": ["/src/pages/*"],
            "@Components/*": ["/src/components/*"],
            "@Assets/*": ["/src/assets/*"],
            "@Styles/*": ["/src/styles/*"],
            "@Utils/*": ["/src/utils/*"]
        }
    },
    "include": ["./src", "tsconfig.json"],
    "exclude": ["build, node_modules"]
}